<h1>Lista de Personas</h1>

<table class = "table">
  <thead>
     <tr> 
       <th scope ="col">#</th>
       <th scope ="col">Identificacion</th>
       <th scope ="col">Nombre</th>
       <th scope ="col">Apellido</th>
       <th scope ="col">Correo</th>
       <th scope ="col">Telefono</th>
     </tr>
  </thead>
  <tbody>
    <%= render @people %>
  </tbody>
</table>

<%= link_to "agregar nueva persona", people_new_path %> 